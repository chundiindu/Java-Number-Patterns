import java.io.*;
import java.lang.*;

class GFG {
    
    public static int fun(int n)
    {
       for(int i=1,p=0;i<=n;i++,p+=2)
       {
           for(int j=1;j<=i;j++)
           {
               System.out.print(p+" ");
           }
           System.out.println();
       }
       return n;
    }
	public static void main (String[] args) {
	    
	    fun(5);
		//code
	}
}
