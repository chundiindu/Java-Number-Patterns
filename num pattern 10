 import java.io.*;
import java.lang.*;

class GFG {
    
    public static int fun(int n)
    {
       for(int i=1;i<=n;i++)
       {
           int p=5;
           for(int j=1;j<=i;j++)
           {
               System.out.print(p--+" ");
           }
           System.out.println();
       }
       return n;
    }
	public static void main (String[] args) {
	    
	    fun(5);
		//code
	}
  }
